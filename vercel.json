{
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "src/css/output.css",
        "use": "@vercel/static"
      },
      {
        "src": "public/dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.html"
      }
    ]
  }
  